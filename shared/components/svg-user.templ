package components

import "strconv"

templ SVGUser(color string, width, height int) {
	<svg
		xmlns="http://www.w3.org/2000/svg"
		viewBox="0 0 24 24"
		width={ strconv.Itoa(width) }
		height={ strconv.Itoa(height) }
		fill={ color }
	>
		<circle cx="12" cy="8" r="4"></circle>
		<path d="M4 20c0-4 4-6 8-6s8 2 8 6"></path>
	</svg>
}
